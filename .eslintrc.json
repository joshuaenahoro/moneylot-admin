{
  "extends": [
    "next/core-web-vitals",
    // eslint-config-prettier: Turn off all rules that are unnecessary or might conflict with Prettier
    "prettier"
  ],
  "rules": {
    // Disallow extra closing tags for components without children
    "react/self-closing-comp": [
      "error",
      {
        "component": true,
        "html": true
      }
    ]
  }
}
